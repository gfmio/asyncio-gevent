{
  "name": "Python 3",
  "build": {
    "dockerfile": "Dockerfile",
    "context": "..",
    "args": {
      "VARIANT": "3.11",
      "INSTALL_NODE": "false",
      "NODE_VERSION": "lts/*"
    }
  },
  "features": {},
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.debugpy",
        "ms-python.vscode-pylance",
        "editorconfig.editorconfig",
        "charliermarsh.ruff",
        "task.vscode-task"
      ],
      "settings": {}
    }
  },
  "postCreateCommand": "task devcontainer:postCreateCommand",
  "containerUser": "vscode",
  "remoteUser": "vscode"
}
